using System;

namespace App
{
    internal class Programm
    {
        static void Main(string[] args)
        {
            int cycleBegining = 5;
            int cycleEnding = 100;
            int cycleStep = 7;

            for (int i = cycleBegining; i < cycleEnding; i += cycleStep)
            {
                Console.WriteLine(i);
            }
        }
    }
}
