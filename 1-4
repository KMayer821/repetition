using System;

namespace App
{
    internal class Programm
    {
        static void Main(string[] args)
        {
            int numberOfImages = 52;
            int imagesInRaw = 3;
            int remainesOfRaws = numberOfImages % imagesInRaw;
            int numberOfRaws = numberOfImages / imagesInRaw;

            Console.WriteLine($"Всего строк: {numberOfRaws}, остаток: {remainesOfRaws}");
        }
    }
}
